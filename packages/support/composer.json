{
    "name": "filament/support",
    "license": "MIT",
    "homepage": "https://github.com/laravel-filament/filament",
    "support": {
        "issues": "https://github.com/laravel-filament/filament/issues",
        "source": "https://github.com/laravel-filament/filament"
    },
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/filesystem": "^8.0",
        "illuminate/support": "^8.0",
        "livewire/livewire": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "Filament\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Filament\\SupportServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
